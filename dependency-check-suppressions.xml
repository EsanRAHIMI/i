<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
    Suppression file for OWASP Dependency Check
    Add suppressions for false positives here
    -->
    
    <!-- Example suppression for a false positive -->
    <!--
    <suppress>
        <notes><![CDATA[
        This is a false positive because...
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/.*$</packageUrl>
        <cve>CVE-2021-XXXXX</cve>
    </suppress>
    -->
    
    <!-- Suppress development dependencies that don't affect production -->
    <suppress>
        <notes><![CDATA[
        Development dependencies are not included in production builds
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/(jest|@testing-library|@types).*$</packageUrl>
    </suppress>
    
    <!-- Suppress Python test dependencies -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not included in production
        ]]></notes>
        <packageUrl regex="true">^pkg:pypi/(pytest|coverage|mock).*$</packageUrl>
    </suppress>
</suppressions>